    "react-native-vector-icons": "^10.2.0",
    "react-native-video": "^6.7.0",
    "react-native-vision-camera": "^4.6.3",
    "react-native-webview": "^13.13.2",
    "react-native-wifi-reborn": "^4.13.5",
